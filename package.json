{
  "name": "command-decorator",
  "version": "0.1.0",
  "description": "Incorporates Command Pattern using ES7 decorators.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/emotz/command-decorator.git"
  },
  "bugs": {
    "url": "https://github.com/emotz/command-decorator/issues"
  },
  "homepage": "https://github.com/emotz/command-decorator#readme",
  "author": "emotz",
  "keywords": [
    "command",
    "pattern",
    "decorator"
  ],
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "clean": "rimraf lib .nyc_output coverage",
    "lint": "tslint --force --format verbose \"src/**/*.ts\"",
    "build": "npm run clean && npm run lint && echo Using TypeScript && tsc --version && tsc --pretty",
    "test": "npm run build && mocha --compilers ts:ts-node/register",
    "coverage": "nyc --reporter=text --reporter=html --reporter=lcov mocha --compilers ts:ts-node/register",
    "watch": "npm run build -- --watch",
    "watch:test": "npm run test -- --watch --reporter min",
    "postinstall": "postinstall-build lib \"npm run build\""
  },
  "dependencies": {
    "postinstall-build": "^3.0.0"
  },
  "devDependencies": {
    "@types/chai": "^3.0.0",
    "@types/mocha": "^2.0.0",
    "@types/node": "^7.0.0",
    "chai": "^3.0.0",
    "mocha": "^3.0.0",
    "nyc": "^10.0.0",
    "rimraf": "^2.0.0",
    "ts-node": "^3.0.0",
    "tslint": "^5.0.0",
    "typescript": "^2.0.0",
    "coveralls": "^2.0.0"
  },
  "engines": {
    "node": ">=7.0.0"
  },
  "nyc": {
    "include": [
      "src/*.ts"
    ],
    "exclude": [
      "lib"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [],
    "all": true
  }
}
